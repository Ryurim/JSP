<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd" id="WebApp_ID" version="5.0">
  <display-name>bbsModel2</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  <context-param>
    <param-name>Driver</param-name>
    <param-value>org.mariadb.jdbc.Driver</param-value>
  </context-param>
  <context-param>
    <param-name>URL</param-name>
    <param-value>jdbc:mariadb://localhost:3306/maria</param-value>
  </context-param>
  <context-param>
    <param-name>dbId</param-name>
    <param-value>root</param-value>
  </context-param>
  <context-param>
    <param-name>dbPwd</param-name>
    <param-value>1234</param-value>
  </context-param>
  <servlet>
    <description></description>
    <display-name>FileUpload</display-name>
    <servlet-name>FileUpload</servlet-name>
    <servlet-class>fileupload.FileUpload</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FileUpload</servlet-name>
    <url-pattern>/FileUpload</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ListController</servlet-name>
    <servlet-class>controller.ListController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ListController</servlet-name>
    <url-pattern>/bbs/list.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ViewController</servlet-name>
    <servlet-class>controller.ViewController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ViewController</servlet-name>
    <url-pattern>/bbs/view.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ModifyController</display-name>
    <servlet-name>ModifyController</servlet-name>
    <servlet-class>controller.ModifyController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ModifyController</servlet-name>
    <url-pattern>/bbs/modify.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>LoginController</display-name>
    <servlet-name>LoginController</servlet-name>
    <servlet-class>controller.LoginController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginController</servlet-name>
    <url-pattern>/login/login.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>LoginOkController</display-name>
    <servlet-name>LoginOkController</servlet-name>
    <servlet-class>controller.LoginOkController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginOkController</servlet-name>
    <url-pattern>/login/login_ok.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>LogOutController</display-name>
    <servlet-name>LogOutController</servlet-name>
    <servlet-class>controller.LogOutController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogOutController</servlet-name>
    <url-pattern>/login/logout.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>RegistController</display-name>
    <servlet-name>RegistController</servlet-name>
    <servlet-class>controller.RegistController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegistController</servlet-name>
    <url-pattern>/bbs/regist_ok.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>DeleteController</display-name>
    <servlet-name>DeleteController</servlet-name>
    <servlet-class>controller.DeleteController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DeleteController</servlet-name>
    <url-pattern>/bbs/delete.do</url-pattern>
  </servlet-mapping>
</web-app>